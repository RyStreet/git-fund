content-end navStyles\"\n    <Navbar style={{justifyContent:\"space-between\"}} className='nav sticky-top gap-5' collapseOnSelect expand=\"lg\" bg=\"light\" >\n      <Container className='m-auto' style={{justifyContent:\"space-between\"}}>\n        <div><Navbar.Brand as={Link} to=\"/\"><img src={logo} className=\"logo \" size='small' alt='gitfund logo'/></Navbar.Brand></div>\n        <Navbar.Toggle aria-controls=\"responsive-navbar-nav\" />\n        <Navbar.Collapse id=\"responsive-navbar-nav\">\n        <Nav className=\"me-auto \"></Nav>\n          <Nav className='navStyles'> \n            {Auth.loggedIn() ? (\n              <>\n                <Nav.Link as={Link} to=\"/\">Home</Nav.Link> \n                <Nav.Link as={Link} to=\"/profile\">Profile</Nav.Link>\n                <Nav.Link as={Link} onClick={handleLogout}>Logout</Nav.Link>\n              </> \n            ) : (\n              <>\n                <Nav.Link as={Link} to=\"/\">Home</Nav.Link>\n                <Nav.Link as={Link} to=\"/profile\">Profile</Nav.Link>\n                <Nav.Link as={Link} to=\"/login\">Login</Nav.Link>\n                <Nav.Link as={Link} to=\"signup\">Sign Up</Nav.Link>\n              </>\n            )}            \n          </Nav>\n        </Navbar.Collapse>\n      </Container>\n    </Navbar>\n  );\n}\n\nexport default NavBar;","D:\\git-fund\\client\\src\\pages\\SingleProject.js",["114","115","116","117","118","119","120"],"import React, { useState } from 'react';\nimport Button from 'react-bootstrap/Button';\nimport Modal from 'react-bootstrap/Modal';\nimport { Link, useNavigate } from 'react-router-dom';\nimport { useParams } from 'react-router-dom';\nimport { useMutation, useQuery } from '@apollo/client';\nimport Donate from '../components/Donate'\nimport Collaborate from '../components/Collaborate'\nimport Collaborators from '../components/Collaborators';\nimport Donations from '../components/Donations';\n\nimport { QUERY_ME } from '../utils/queries';\nimport { QUERY_USER } from '../utils/queries';\nimport { QUERY_SINGLE_PROJECT } from '../utils/queries';\nimport { REMOVE_PROJECT } from '../utils/mutations';\n\nimport Auth from '../utils/auth';\n\nfunction SingleProject() {\n  const navigate = useNavigate();\n\n  const [ deleteBtn, setDeleteBtn ] = useState(false);\n  const handleClose = () => setDeleteBtn(false);\n  const handleShow = () => setDeleteBtn(true);\n\n  const [removeProject, {error}] = useMutation(REMOVE_PROJECT);\n\n  const { projectId } = useParams();\n  const { loading, data } = useQuery(QUERY_SINGLE_PROJECT, {\n    variables: { projectId: projectId }\n  });\n\n  const project = data?.project || {};\n  \n  if (!Auth.loggedIn()) {\n    return (\n      <div style={{height: \"400px\"}}>\n      <Link to={'/login'} className=\"textDecNone\">\n       <div className=\"msgScreen\">\n          <h2>Login or sign up to view user profiles</h2>\n        </div>\n      </Link>\n      </div>\n    )\n  }\n  if (loading) {\n    return <div>Loading...</div>\n  }\n\n  // For total donations\n  let donations = project.fundingEarned\n  let total = 0\n\n  for (let i = 0; i < donations.length; i++){\n   const loopDonations = donations[i].amount\n   total += loopDonations\n  }\n\n  // To delete project\n  const handleRemoveProject = async(event) => {\n    event.preventDefault();\n    console.log(\"Project Removed!\")\n\n    try {\n      const {data} = await removeProject({\n        variables: {\n          projectId: project._id\n        }\n      })\n      navigate(\"/profile\", {replace: true});\n      navigate(0);\n    } catch (err) {\n      console.error(err)\n    }\n  };\n\n  const myProject = () => {\n    const me = JSON.stringify(Auth.getProfile().data.username)\n    const projectCreator = JSON.stringify(project.creator)\n    if (me === projectCreator) {\n      console.log(\"matching\")\n      return true;\n    } else {\n      console.log(\"not matching\")\n      return false;\n    } \n  }\n  const isMyProject = myProject()\n\n  return (\n    <>\n      <div className='project'>\n        <div className='project-section1'>\n          <h1>{project.title}</h1>\n          <h4>By: <Link className=\"textDecNone\" to={`/profiles/${project.creator}`}>{project.creator}</Link></h4>\n          <h5>{project.description}</h5>\n          {/* <h5>Project Repository: {project.repo}</h5> */}\n          <a className=\"ghIcon\" style={{textDecoration:\"none\", color:\"black\"}} href={project.repo} target=\"_blank\" rel=\"noreferrer\" >\n            <i className=\"github icon huge\"></i>\n          </a>\n        </div>  \n\n        <br/>\n\n        <div className='project-section3'>          \n          <h4>Funding Total: ${total}</h4>\n          <h4>Funding Goal: ${project.fundingGoal}</h4>\n          {/* <Progress percent=\"80\" inverted progress success/> */}\n          {!isMyProject == true ? (\n            <Donate projectId={project._id} className='button donate-btn' />\n          ) : (\n            <div></div>\n          )}\n        </div>\n\n        <div className='project-section2'>\n          <h4>Collaborators:</h4>\n          <h5><Collaborators collaborators={project.collaborators}/></h5>\n          {!isMyProject == true ? (\n            <Collaborate projectId={project._id} className='button' />\n          ) : (\n            <div></div>\n          )}\n        </div>\n\n        {isMyProject == true ? (\n          <div id=\"editAndDelBtns\">\n          <div >\n            <Link to= {`/projects/${project._id}/edit`} >\n            <Button className='edit-project-btn' variant='warning'>Edit</Button>\n            </Link>\n          </div>\n  \n          <div className='delete-project-btn'>\n            <Button variant=\"danger\" onClick={handleShow}>Delete</Button>\n            <Modal show={deleteBtn} onHide={handleClose}>\n              <Modal.Header closeButton>\n                <Modal.Title>Delete Project</Modal.Title>\n              </Modal.Header>\n              <Modal.Body>Are you sure you want to permanently delete this project?</Modal.Body>\n              <Modal.Footer>\n                <Button variant=\"secondary\" onClick={handleClose}>\n                  Cancel\n                </Button>\n                <Button variant=\"danger\" onClick={handleRemoveProject}>\n                  Yes\n                </Button>\n              </Modal.Footer>\n            </Modal>\n\n\n          </div>\n          </div>\n          ) : (\n          <div></div>\n        )}\n      </div>\n      \n      <div > \n        <Donations fundingEarned={project.fundingEarned}/>\n      </div> \n    </>\n  )\n};\n\nexport default SingleProject;","D:\\git-fund\\client\\src\\pages\\Profile.js",["121","122","123","124","125","126","127","128"],"import React,  {useState } from \"react\";\nimport { Link, Navigate, useNavigate, useParams } from \"react-router-dom\";\nimport Auth from '../utils/auth';\nimport { useQuery } from \"@apollo/client\";\nimport { QUERY_ME, QUERY_USER } from \"../utils/queries\"\nimport ProjectCards from \"../components/ProjectCards\";\nimport { useMutation } from \"@apollo/client\";\nimport { EDIT_PROFILE } from '../utils/mutations'\n\nimport EdiText from 'react-editext';\n\n\nfunction Profile() {  \n  const navigate = useNavigate()\n\n  // For bio \n  const [bio, setBio] = useState('');\n  const [editBio, {error}] = useMutation(EDIT_PROFILE);\n\n  const { username: userParam } = useParams();\n  const {loading, data} = useQuery(userParam ? QUERY_USER : QUERY_ME, {\n    variables: { username: userParam }\n  });\n\n  const user = data?.me || data?.user || []\n  \n  const projects = user.projects\n  const collabProjects = user.collabProjects\n\n  if (Auth.loggedIn() && Auth.getProfile().data.username === userParam) {\n    return <Navigate to=\"/profile\" />\n  }\n  if (!Auth.loggedIn()) {\n    return (\n      <div style={{height: \"400px\"}}>\n      <Link  to={'/login'} className=\"textDecNone\">\n        <div className=\"msgScreen\">\n          <h2>Login or sign up to view user profiles</h2>\n        </div>\n      </Link>\n      </div>\n    )\n  }\n  if (loading) {\n    return <div>Loading...</div>;\n  }\n\n  // For saving and editing bio\n  const handleSaveBio = async (bio) => {\n    \n    try {\n      const {data} = await editBio({\n        variables: {\n          userId: user._id,\n          bio\n        }\n      })\n      setBio(\"\");\n     \n      navigate(0)\n    } catch (err) {\n      console.error(err)\n    }\n  };\n\n  const handleChange = (event) => {\n    let { name, value} = event.target;\n\n    if(name= \"bio\") {\n      setBio(value)\n    }\n  }\n\n  return (\n    <div className=\"profileMargins\">\n\n      <div className=\"project\" style={{marginBottom: 35, width: \"80%\", alignContent:\"center\", backgroundColor:\"#F7DFB8\"}}>\n        <h2>{userParam ? `Now Viewing ${user.username}'s` : `Your`} Profile</h2>\n        <div style={{marginBottom: 20}}>\n        {/* <h5 style={{marginBottom:10}} href={user.email}>Email: {user.email}</h5> */}\n\n        <a style={{textDecoration:\"none\", color:\"grey\"}} href=\"mailto: {user.email}\">\n        <i className=\"mail icon huge\" ></i>\n        </a>\n       \n\n        <a style={{textDecoration:\"none\"}} href={user.linkedin} target=\"_blank\" rel=\"noreferrer\">\n        <i className=\"linkedin icon huge\" ></i>\n        </a>\n        \n        \n        \n        <a style={{textDecoration:\"none\", color:\"black\"}} href={user.github} target=\"_blank\" rel=\"noreferrer\" >\n        <i className=\"github icon huge\"></i>\n        </a>\n\n        \n        </div >\n        {!userParam ? (\n          <div>\n            <h5 style={{maxWidth:\"70%\", marginBottom:15}}>Bio: {user.bio}</h5>\n            {/* <EdiText \n              name=\"bio\" type='textarea' value={bio} \n              editButtonContent=\"Edit\"\n              inputProps={{ placeholder:\"Edit your bio here\", style: {maxWidth:\"70%\"}, rows: 3 }}\n              onSave={handleSaveBio} onChange={handleChange}\n     